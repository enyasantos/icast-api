{
    "version": 2,
    "env": {
        "DATABASE_URL": "file:./dev.db",
        "UPLOAD_LOCATION": "./uploads/avatar",
        "MAX_FILE_SIZE": "50mb"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE"
            ]
        }
    ]
}